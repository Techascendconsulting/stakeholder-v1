  "enhancement_rules": {
    "add_filler": {
      "trigger": "sentence_start",
      "probability": 0.3,
      "position": "before",
      "source": "personality.fillers"
    },
    "add_thinking_pause": {
      "trigger": ["question_response", "complex_topic"],
      "duration_range": [300, 600],
      "position": "before"
    },
    "adjust_emotion": {
      "trigger": "user_frustration_detected",
      "apply": "personality.emotionalRanges.empathetic",
      "strength": 0.9
    },
    "add_greeting_pause": {
      "trigger": "greeting",
      "duration": "personality.pausePatterns.afterGreeting",
      "position": "after"
    },
    "emphasize_expertise": {
      "trigger": "expertise_topic",
      "emphasis_level": "moderate",
      "apply_to": "personality.emphasis_words"
    }
  },

  "context_mappings": {
    "user_confused": {
      "emotion": "empathetic",
      "pace": "slower", 
      "filler_frequency": "increased",
      "pause_frequency": "increased"
    },
    "technical_discussion": {
      "emotion": "thoughtful",
      "pace": "measured",
      "filler_frequency": "reduced",
      "emphasis": "technical_terms"
    },
    "greeting": {
      "emotion": "friendly",
      "pace": "normal",
      "filler_frequency": "reduced",
      "greeting_style": "casual"
    },
    "problem_solving": {
      "emotion": "confident",
      "pace": "measured",
      "filler_frequency": "normal",
      "emphasis": "solutions"
    },
    "user_frustrated": {
      "emotion": "empathetic",
      "pace": "slower",
      "filler_frequency": "reduced",
      "tone": "calming"
    }
  },

