  "enhancement_rules": {
    "add_filler": {
      "trigger": "sentence_start",
      "probability": 0.3,
      "position": "before",
      "source": "personality.fillers"
    },
    "add_thinking_pause": {
      "trigger": ["question_response", "complex_topic"],
      "duration_range": [300, 600],
      "position": "before"
    },
    "adjust_emotion": {
      "trigger": "user_frustration_detected",
      "apply": "personality.emotionalRanges.empathetic",
      "strength": 0.9
    },
    "add_greeting_pause": {
      "trigger": "greeting",
      "duration": "personality.pausePatterns.afterGreeting",
      "position": "after"
    },
    "emphasize_expertise": {
      "trigger": "expertise_topic",
      "emphasis_level": "moderate",
      "apply_to": "personality.emphasis_words"
    }
  },

  "context_mappings": {
    "user_confused": {
      "emotion": "empathetic",
      "pace": "slower", 
      "filler_frequency": "increased",
      "pause_frequency": "increased"
    },
    "technical_discussion": {
      "emotion": "thoughtful",
      "pace": "measured",
      "filler_frequency": "reduced",
      "emphasis": "technical_terms"
    },
    "greeting": {
      "emotion": "friendly",
      "pace": "normal",
      "filler_frequency": "reduced",
      "greeting_style": "casual"
    },
    "problem_solving": {
      "emotion": "confident",
      "pace": "measured",
      "filler_frequency": "normal",
      "emphasis": "solutions"
    },
    "user_frustrated": {
      "emotion": "empathetic",
      "pace": "slower",
      "filler_frequency": "reduced",
      "tone": "calming"
    }
  },

  "ssml_templates": {
    "greeting": {
      "pattern": "{pause_short}{filler?}{pause_medium}{greeting_phrase}{pause_after_greeting}",
      "emotional_weight": 0.8
    },
    "explanation": {
      "pattern": "{pause_thinking?}{transition?}{pause_short}{emphasis_start?}{main_content}{emphasis_end?}{pause_medium}",
      "emotional_weight": 0.6
    },
    "acknowledgment": {
      "pattern": "{filler?}{pause_short}{validation_phrase}{pause_medium}{next_action?}",
      "emotional_weight": 0.7
    },
    "question_response": {
      "pattern": "{pause_thinking}{filler?}{pause_short}{response_content}{pause_medium}",
      "emotional_weight": 0.6
    },
    "technical_explanation": {
      "pattern": "{pause_thinking}{transition}{pause_short}{technical_content}{pause_long}",
      "emotional_weight": 0.5
    }
